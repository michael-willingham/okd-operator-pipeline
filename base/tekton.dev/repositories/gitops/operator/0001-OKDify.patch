diff --git a/Dockerfile b/Dockerfile
index 6ec4196..ddc4f54 100644
--- a/Dockerfile
+++ b/Dockerfile
@@ -1,5 +1,5 @@
 # Build the manager binary
-FROM golang:1.21 as builder
+FROM quay.io/projectquay/golang:1.21 as builder

 WORKDIR /workspace
 # Copy the Go Modules manifests
diff --git a/Makefile b/Makefile
index 3e2efd8..433c2ca 100644
--- a/Makefile
+++ b/Makefile
@@ -172,7 +172,7 @@ undeploy: ## Undeploy controller from the K8s cluster specified in ~/.kube/confi

 CONTROLLER_GEN = $(shell pwd)/bin/controller-gen
 controller-gen: ## Download controller-gen locally if necessary.
-	$(call go-get-tool,$(CONTROLLER_GEN),sigs.k8s.io/controller-tools/cmd/controller-gen@v0.4.1)
+	@$(call go-get-tool,$(CONTROLLER_GEN),sigs.k8s.io/controller-tools/cmd/controller-gen@v0.4.1)

 KUSTOMIZE = $(shell pwd)/bin/kustomize
 kustomize: ## Download kustomize locally if necessary.
@@ -196,6 +196,7 @@ endef

 .PHONY: bundle
 bundle: manifests kustomize ## Generate bundle manifests and metadata, then validate generated files.
+	@sed -i 's|containerImage: .*|containerImage: $(IMAGE)|g' config/manifests/bases/gitops-operator.clusterserviceversion.yaml
 	operator-sdk generate kustomize manifests -q
 	cd config/manager && $(KUSTOMIZE) edit set image controller=$(IMG)
 	$(KUSTOMIZE) build config/manifests | operator-sdk generate bundle -q --overwrite --version $(VERSION) $(BUNDLE_METADATA_OPTS)
@@ -267,3 +268,35 @@ catalog-build: opm ## Build a catalog image.
 .PHONY: catalog-push
 catalog-push: ## Push a catalog image.
 	$(MAKE) docker-push IMG=$(CATALOG_IMG)
+
+####### OKD #######
+CONSOLE_IMAGE ?= quay.io/redhat-developer/gitops-console-plugin
+CONSOLE_IMAGE_TAG ?= v0.1.0
+BACKEND_IMAGE ?= quay.io/redhat-developer/gitops-backend
+BACKEND_IMAGE_TAG ?= v0.0.1
+BACKEND_IMG ?= $(BACKEND_IMAGE):$(BACKEND_IMAGE_TAG)
+BUILD_CMD ?= podman
+
+.PHONY: okd-install okd-lint okd-build okd-test okd-deploy okd-bundle
+
+okd-install:
+	@go mod download
+	@sed -i 's|DefaultConsoleImage = ".*"|DefaultConsoleImage = "$(CONSOLE_IMAGE)"|g' common/common.go
+	@sed -i 's|DefaultConsoleVersion = ".*"|DefaultConsoleVersion = "$(CONSOLE_IMAGE_TAG)"|g' common/common.go
+	@sed -i 's|backendImage                    string = ".*"|backendImage                    string = "$(BACKEND_IMG)"|g' controllers/gitopsservice_controller.go
+
+okd-lint:
+	@echo "Project has no lint"
+
+okd-test: #test
+	@echo "Tests require running in an OKD cluster"
+
+okd-build: build
+
+okd-deploy:
+	@$(BUILD_CMD) build -t $(IMG) .
+	@$(BUILD_CMD) push $(IMG)
+
+okd-bundle: bundle
+	@$(BUILD_CMD) build -f bundle.Dockerfile -t $(BUNDLE_IMG) .
+	@$(BUILD_CMD) push $(BUNDLE_IMG)
